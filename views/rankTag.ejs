<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <link rel="stylesheet" type="text/css" href="/css/ranks.css">
  <title>Rank</title>
</head>

<body>
  <div class="content">
    <h1>Ranks</h1>
    <div id="divisions-tag">
    </div>
    <div id="informations">
      <h2>Contenders</h2>
      Total:<%=pis.length %>
    </div>
    <div id="contenders" class="tagRank">
      <div class="tbl-header">
        <table cellpadding="0" cellspacing="0" border="0">
          <thead>
            <tr>
              <th></th>
              <th>Name</th>
              <th>Points</th>
            </tr>
          </thead>
        </table>
      </div>
      <div class="tbl-content">
        <table cellpadding="0" cellspacing="0" border="0">
          <tbody>
            <% pis.forEach(function(data) { %>
              <tr>
                <td>
                  <div class="champion-tag"><img src="/images/<%= data.name %>.png" /></div>
                </td>
                <td>
                  <a href="/showTag?name=<%= data.name %>">
                    <%= data.name %>
                  </a>
                </td>
                <td>
                  <h4>
                    <%= data.points %>
                  </h4>
                </td>
              </tr>
              <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <script type="text/javascript" src="/scripts/champion.js"></script>
  <script>championTag();</script>
</body>

</html>